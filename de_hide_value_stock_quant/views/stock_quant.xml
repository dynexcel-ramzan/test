<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!--Hide Value in Stock Quant list view-->
        <record id="inherit_stock_quant_tree_view_editable" model="ir.ui.view">
            <field name="name">inherit.stock.quant.tree.view.editable</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock_account.view_stock_quant_tree_editable_inherit"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='value']" position="attributes">
                    <attribute name="attrs">{}</attribute>
                    <attribute name="invisible">1</attribute>
                </xpath>

            </field>
        </record>
    </data>
</odoo>